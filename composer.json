{
    "name": "elsevier-io/json-schema-php-generator",
    "description": "Lib to generate a PHP representation of the data structures in a JSON Schema",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andy Clyde",
            "email": "a.clyde@elsevier.com"
        }
    ],
    "repositories": [{
        "type": "package",
        "package": {
            "name": "json-schema/JSON-Schema-Test-Suite",
            "version": "1.2.0",
            "source": {
                "url": "https://github.com/json-schema/JSON-Schema-Test-Suite",
                "type": "git",
                "reference": "1.2.0"
            }
        }
    }],
    "require": {
        "php" : "^5.5 || ^7.0",
        "league/flysystem": "^1.0",
        "nette/php-generator": "2.3.*",
        "nette/utils": "2.3.*"
    },
    "require-dev": {
        "json-schema/JSON-Schema-Test-Suite": "1.2.0",
        "phpunit/phpunit": "^4.8.36",
        "hamcrest/hamcrest-php": "^2.0"
    },
    "autoload": {
        "psr-4": { "Elsevier\\JSONSchemaPHPGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Elsevier\\JSONSchemaPHPGenerator\\Tests\\": "tests/" }
    },
    "scripts": {
        "test" : "vendor/bin/phpunit"
    }
}
